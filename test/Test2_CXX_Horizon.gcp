#include <iostream>
#include "../include/gcd.h"

int main()
{
#pragma gcd begin
  P = VecN3(1,1,0);
  r = 1;
  S = e0-0.5*r*r*einf;
  C = S^(P+(P.S)*einf);
  ?Cntr = C*einf*C;
#pragma gcd end
  std::cout << "Kreismittelpunkt: " << Cntr[E1] << "," << Cntr[E2] << "," << Cntr[E3];

  return 0;
}
