#include <iostream>
#include <gpc.h>

int main()
{
#pragma gcd begin
  P = VecN3(1,1,0);
  r = 1;
  S = e0-0.5*r*r*einf;
  C = S^(P+(P.S)*einf);
  ?homogenCenter = C*einf*C;
  ?scale = -homogenCenter.einf;
  ?euclidCenter = homogenCenter / scale;
#pragma gcd end
  return !(mv_get_bladecoeff(euclidCenter,e1) == 0.5f &&
	   mv_get_bladecoeff(euclidCenter,e2) == 0.5f &&
	   mv_get_bladecoeff(euclidCenter,e3) == 0.0f);
}
